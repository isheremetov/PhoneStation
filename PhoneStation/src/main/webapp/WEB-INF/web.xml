  </filter>
  <filter>
    <display-name>ServicesFilter</display-name>
    <filter-name>ServicesFilter</filter-name>
    <filter-class>by.gsu.isheremetov.phonestation.filters.ServicesFilter</filter-class>
  </filter>
  <filter>
    <description>
		</description>
    <display-name>DialoguesFilter</display-name>
    <filter-name>DialoguesFilter</filter-name>
    <filter-class>by.gsu.isheremetov.phonestation.filters.DialoguesFilter</filter-class>
