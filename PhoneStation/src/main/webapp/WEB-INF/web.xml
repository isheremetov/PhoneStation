    <filter-name>CabinetFilterAuthorization</filter-name>
    <filter-class>by.gsu.isheremetov.phonestation.filters.CabinetFilterAuthorization</filter-class>
  </filter>
  <filter>
    <display-name>AdministratorFilter</display-name>
    <filter-name>AdministratorFilter</filter-name>
