    <filter-name>IndexFilter</filter-name>
    <url-pattern>/index.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>CabinetFilterAuthorization</filter-name>
    <url-pattern>/cabinet.jsp</url-pattern>
    <url-pattern>/jsp/user/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>AdministratorFilter</filter-name>
    <url-pattern>/admin.jsp</url-pattern>
    <url-pattern>/jsp/admin/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>ServicesFilter</filter-name>
    <url-pattern>/jsp/user/services.jsp</url-pattern>
